		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let response = pm.response.json();",
							"",
							"let token = response.token;",
							"",
							"pm.globals.set(\"faynoSyncToken\", token);",
							"",
							"console.log(\"faynoSyncToken saved to globals:\", token);",
							""
						],
						"type": "text/javascript",
						"packages": {}
